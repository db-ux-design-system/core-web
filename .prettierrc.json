{
	"trailingComma": "none",
	"bracketSpacing": true,
	"overrides": [
		{
			"files": ["*.js", "*.jsx", "*.cjs", "*.ts", "*.tsx"],
			"options": {
				"bracketSameLine": true
			}
		},
		{
			"files": ["*.sh", ".env"],
			"options": {
				"parser": "sh"
			}
		},
		{
			"files": ["*.scss"],
			"options": {
				"trailingComma": "none"
			}
		}
	],
	"endOfLine": "auto"
}
