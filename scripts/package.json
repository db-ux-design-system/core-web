{
  "name": "scripts",
  "version": "0.0.0",
  "type": "module",
  "description": "contains scripts for monorepo",
  "scripts": {
    "build": "npm-run-all build:foundations build:components build:migration build:stylelint",
    "build-outputs": "npm-run-all -p build-outputs:*",
    "build-outputs:components": "npm run copy-output --workspace=@db-ux/core-components",
    "build-outputs:foundations": "npm run copy-build --workspace=@db-ux/core-foundations",
    "build-outputs:migration": "npm run copy-build --workspace=@db-ux/core-migration",
    "build-outputs:ngx-core-components": "npm run build --workspace=@db-ux/ngx-core-components",
    "build-outputs:react-core-components": "npm run build --workspace=@db-ux/react-core-components",
    "build-outputs:stencil-components": "npm run build --workspace=@db-ux/wc-core-components",
    "build-outputs:stylelint": "npm run copy-output --workspace=@db-ux/core-stylelint",
    "build-outputs:v-core-components": "npm run build --workspace=@db-ux/v-core-components",
    "build-outputs:wc-core-components": "npm run build --workspace=@db-ux/wc-core-components",
    "build-showcases": "npm-run-all -p build-showcases:*",
    "build-showcases:angular": "npm run build --workspace=angular-showcase",
    "build-showcases:angular-ssr": "npm run build --workspace=angular-ssr-showcase",
    "build-showcases:next": "npm run build --workspace=next-showcase",
    "build-showcases:nuxt": "npm run generate --workspace=nuxt-showcase",
    "build-showcases:patternhub": "npm run build --workspace=patternhub",
    "build-showcases:react": "npm run build --workspace=react-showcase",
    "build-showcases:stencil": "npm run build --workspace=stencil-showcase",
    "build-showcases:vue": "npm run build --workspace=vue-showcase",
    "build:components": "npm run build --workspace=@db-ux/core-components",
    "build:foundations": "npm run build --workspace=@db-ux/core-foundations",
    "build:migration": "npm run build --workspace=@db-ux/core-migration",
    "build:stylelint": "npm run build --workspace=@db-ux/core-stylelint",
    "dev": "tsx dev.js",
    "dev:angular-components": "npm run dev:angular --workspace=@db-ux/core-components",
    "dev:plain-html-components": "npm run dev:html --workspace=@db-ux/core-components",
    "dev:react-components": "npm run dev:react --workspace=@db-ux/core-components",
    "dev:sass": "npm run dev:scss --workspace=@db-ux/core-components",
    "dev:stencil-components": "npm run dev:stencil --workspace=@db-ux/core-components",
    "dev:vue-components": "npm run dev:vue --workspace=@db-ux/core-components",
    "rm-builds:components-build": "rimraf ../packages/components/build",
    "rm-builds:dist": "rimraf -g showcases/**/dist",
    "rm-builds:foundations-build": "rimraf ../packages/foundations/build",
    "rm-builds:output-dist": "rimraf -g ../output/**/dist",
    "rm-builds:output-playwright": "rimraf -g ../output/**/playwright",
    "rm-builds:outputs": "rimraf -g ../output/**/src",
    "rm-builds:patternhub-out": "rimraf -g showcases/patternhub/out",
    "rm-builds:patternhub-pages-code": "rimraf -g ../showcases/patternhub/pages/components/**/code",
    "rm-builds:patternhub-pages-docs": "rimraf -g ../showcases/patternhub/pages/components/**/docs",
    "rm-builds:patternhub-pages-htu": "rimraf -g ../showcases/patternhub/pages/components/**/how-to-use.mdx",
    "rm-builds:patternhub-pages-index": "rimraf -g ../showcases/patternhub/pages/components/**/index.tsx",
    "rm-builds:patternhub-pages-migration": "rimraf -g ../showcases/patternhub/pages/components/**/migration.mdx",
    "rm-builds:patternhub-pages-overview": "rimraf -g ../showcases/patternhub/pages/components/**/overview.tsx",
    "rm-builds:patternhub-pages-properties": "rimraf -g ../showcases/patternhub/pages/components/**/properties.mdx",
    "rm-builds:patternhub-src-ts": "rimraf -g ../showcases/patternhub/components/src/**/*.ts",
    "rm-builds:patternhub-src-tsx": "rimraf -g ../showcases/patternhub/components/src/**/*.tsx",
    "rm-builds:playwright": "rimraf -g output/react/playwright",
    "rm-builds:showcases-build": "rimraf -g  build-showcases/**",
    "rm:builds": "npm-run-all -p rm-builds:*",
    "start-showcase:angular": "npm run dev --workspace=angular-showcase",
    "start-showcase:angular-ssr": "npm run dev --workspace=angular-ssr-showcase",
    "start-showcase:next": "npm run dev --workspace=next-showcase",
    "start-showcase:nuxt": "npm run dev --workspace=nuxt-showcase",
    "start-showcase:patternhub": "npm run dev --workspace=patternhub",
    "start-showcase:react": "npm run dev --workspace=react-showcase",
    "start-showcase:stencil": "npm run dev --workspace=stencil-showcase",
    "start-showcase:vue": "npm run dev --workspace=vue-showcase",
    "start:foundations": "npm run start --workspace=@db-ux/core-foundations",
    "test": "npm-run-all -p test:*",
    "test:migration": "npm run test --workspace=@db-ux/core-migration",
    "test:scripts": "vitest run --config vitest.config.ts",
    "update:icon-fonts": "npm-run-all generate:icon-fonts generate:icon-types"
  },
  "dependencies": {
    "postcss-scss": "^4.0.9"
  },
  "devDependencies": {
    "@inquirer/prompts": "7.5.3",
    "find-versions": "^6.0.0",
    "glob": "^11.0.2",
    "inquirer": "^12.6.3",
    "replace-in-file": "^8.3.0"
  }
}
