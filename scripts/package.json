{
  "name": "scripts",
  "version": "0.0.0",
  "type": "module",
  "description": "contains scripts for monorepo",
  "scripts": {
    "build": "npm-run-all build:foundations build:components build:migration build:stylelint",
    "build-outputs": "pnpm --parallel run '/^build-outputs:[^:]*$/'",
    "build-outputs:components": "pnpm run copy-output --filter=@db-ux/core-components",
    "build-outputs:foundations": "pnpm run copy-build --filter=@db-ux/core-foundations",
    "build-outputs:migration": "pnpm run copy-build --filter=@db-ux/core-migration",
    "build-outputs:ngx-core-components": "pnpm run build --filter=@db-ux/ngx-core-components",
    "build-outputs:react-core-components": "pnpm run build --filter=@db-ux/react-core-components",
    "build-outputs:stencil-components": "pnpm run build --filter=@db-ux/wc-core-components",
    "build-outputs:stylelint": "pnpm run copy-output --filter=@db-ux/core-stylelint",
    "build-outputs:v-core-components": "pnpm run build --filter=@db-ux/v-core-components",
    "build-outputs:wc-core-components": "pnpm run build --filter=@db-ux/wc-core-components",
    "build-showcases": "pnpm --parallel run '/^build-showcases:[^:]*$/'",
    "build-showcases:angular": "pnpm run build --filter=angular-showcase",
    "build-showcases:angular-ssr": "pnpm run build --filter=angular-ssr-showcase",
    "build-showcases:next": "pnpm run build --filter=next-showcase",
    "build-showcases:nuxt": "pnpm run generate --filter=nuxt-showcase",
    "build-showcases:patternhub": "pnpm run build --filter=patternhub",
    "build-showcases:react": "pnpm run build --filter=react-showcase",
    "build-showcases:stencil": "pnpm run build --filter=stencil-showcase",
    "build-showcases:vue": "pnpm run build --filter=vue-showcase",
    "build:components": "pnpm run build --filter=@db-ux/core-components",
    "build:foundations": "pnpm run build --filter=@db-ux/core-foundations",
    "build:migration": "pnpm run build --filter=@db-ux/core-migration",
    "build:stylelint": "pnpm run build --filter=@db-ux/core-stylelint",
    "dev": "tsx dev.js",
    "dev:angular-components": "pnpm run dev:angular --filter=@db-ux/core-components",
    "dev:plain-html-components": "pnpm run dev:html --filter=@db-ux/core-components",
    "dev:react-components": "pnpm run dev:react --filter=@db-ux/core-components",
    "dev:sass": "pnpm run dev:scss --filter=@db-ux/core-components",
    "dev:stencil-components": "pnpm run dev:stencil --filter=@db-ux/core-components",
    "dev:vue-components": "pnpm run dev:vue --filter=@db-ux/core-components",
    "rm-builds:components-build": "rimraf ../packages/components/build",
    "rm-builds:dist": "rimraf --glob showcases/**/dist",
    "rm-builds:foundations-build": "rimraf ../packages/foundations/build",
    "rm-builds:output-dist": "rimraf --glob ../output/**/dist",
    "rm-builds:output-playwright": "rimraf --glob ../output/**/playwright",
    "rm-builds:outputs": "rimraf --glob ../output/**/src",
    "rm-builds:patternhub-out": "rimraf --glob showcases/patternhub/out",
    "rm-builds:patternhub-pages-code": "rimraf --glob ../showcases/patternhub/pages/components/**/code",
    "rm-builds:patternhub-pages-docs": "rimraf --glob ../showcases/patternhub/pages/components/**/docs",
    "rm-builds:patternhub-pages-htu": "rimraf --glob ../showcases/patternhub/pages/components/**/how-to-use.mdx",
    "rm-builds:patternhub-pages-index": "rimraf --glob ../showcases/patternhub/pages/components/**/index.tsx",
    "rm-builds:patternhub-pages-migration": "rimraf --glob ../showcases/patternhub/pages/components/**/migration.mdx",
    "rm-builds:patternhub-pages-overview": "rimraf --glob ../showcases/patternhub/pages/components/**/overview.tsx",
    "rm-builds:patternhub-pages-properties": "rimraf --glob ../showcases/patternhub/pages/components/**/properties.mdx",
    "rm-builds:patternhub-src-ts": "rimraf --glob ../showcases/patternhub/components/src/**/*.ts",
    "rm-builds:patternhub-src-tsx": "rimraf --glob ../showcases/patternhub/components/src/**/*.tsx",
    "rm-builds:playwright": "rimraf --glob output/react/playwright",
    "rm-builds:showcases-build": "rimraf --glob  build-showcases/**",
    "rm:builds": "pnpm --parallel run '/^rm-builds:[^:]*$/'",
    "start-showcase:angular": "pnpm run dev --filter=angular-showcase",
    "start-showcase:angular-ssr": "pnpm run dev --filter=angular-ssr-showcase",
    "start-showcase:next": "pnpm run dev --filter=next-showcase",
    "start-showcase:nuxt": "pnpm run dev --filter=nuxt-showcase",
    "start-showcase:patternhub": "pnpm run dev --filter=patternhub",
    "start-showcase:react": "pnpm run dev --filter=react-showcase",
    "start-showcase:stencil": "pnpm run dev --filter=stencil-showcase",
    "start-showcase:vue": "pnpm run dev --filter=vue-showcase",
    "start:foundations": "pnpm run start --filter=@db-ux/core-foundations",
    "test": "pnpm --parallel run '/^test:[^:]*$/'",
    "test:migration": "pnpm run test --filter=@db-ux/core-migration",
    "test:scripts": "vitest run --config vitest.config.ts",
    "update:icon-fonts": "npm-run-all generate:icon-fonts generate:icon-types"
  },
  "devDependencies": {
    "@inquirer/prompts": "7.8.0",
    "dotenv": "17.2.1",
    "find-versions": "6.0.0",
    "glob": "11.0.3",
    "inquirer": "12.9.0",
    "replace-in-file": "8.3.0",
    "tar": "7.4.3",
    "vitest": "3.2.4"
  }
}
