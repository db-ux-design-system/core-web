{
  "name": "scripts",
  "version": "0.0.0",
  "type": "module",
  "description": "contains scripts for monorepo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/db-ux-design-system/core-web.git"
  },
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "agent": "npm-run-all --parallel agent:00_* --sequential agent:01_*",
    "agent:00_extract-css": "tsx documentation/extract-css-vars.ts",
    "agent:00_mitosis-agent": "npm run generate:agent --workspace=@db-ux/core-components",
    "agent:01_merge_docs": "tsx documentation/merge-component-docs.ts",
    "agent:x00_typedoc": "typedoc --options typedoc.json --tsconfig tsconfig.typedoc.json",
    "build": "npm-run-all build:foundations build:components --parallel build:migration build:stylelint build:agent-cli",
    "build-outputs": "npm-run-all --parallel build-outputs:*",
    "build-outputs:agent-cli": "npm run copy-build --workspace=@db-ux/agent-cli",
    "build-outputs:components": "npm run copy-output --workspace=@db-ux/core-components",
    "build-outputs:foundations": "npm run copy-build --workspace=@db-ux/core-foundations",
    "build-outputs:migration": "npm run copy-build --workspace=@db-ux/core-migration",
    "build-outputs:ngx-core-components": "npm run build --workspace=@db-ux/ngx-core-components",
    "build-outputs:react-core-components": "npm run build --workspace=@db-ux/react-core-components",
    "build-outputs:stylelint": "npm run copy-output --workspace=@db-ux/core-stylelint",
    "build-outputs:v-core-components": "npm run build --workspace=@db-ux/v-core-components",
    "build-outputs:wc-core-components": "npm run build --workspace=@db-ux/wc-core-components",
    "build-showcases": "npm-run-all --parallel build-showcases:*",
    "build-showcases:angular": "npm run build --workspace=angular-showcase",
    "build-showcases:angular-ssr": "npm run build --workspace=angular-ssr-showcase",
    "build-showcases:next": "npm run build --workspace=next-showcase",
    "build-showcases:nuxt": "npm run generate --workspace=nuxt-showcase",
    "build-showcases:patternhub": "npm run build --workspace=patternhub",
    "build-showcases:react": "npm run build --workspace=react-showcase",
    "build-showcases:stencil": "npm run build --workspace=stencil-showcase",
    "build-showcases:vue": "npm run build --workspace=vue-showcase",
    "build-storybooks": "npm-run-all --parallel build-storybooks:*",
    "build-storybooks:_composition": "npm run build --workspace=composition-storybook",
    "build-storybooks:angular": "npm run build --workspace=angular-storybook",
    "build-storybooks:react": "npm run build --workspace=react-storybook",
    "build-storybooks:vue": "npm run build --workspace=vue-storybook",
    "build:agent-cli": "npm run build --workspace=@db-ux/agent-cli",
    "build:components": "npm run build --workspace=@db-ux/core-components",
    "build:foundations": "npm run build --workspace=@db-ux/core-foundations",
    "build:migration": "npm run build --workspace=@db-ux/core-migration",
    "build:stylelint": "npm run build --workspace=@db-ux/core-stylelint",
    "dev": "tsx dev.js",
    "dev:angular-components": "npm run dev:angular --workspace=@db-ux/core-components",
    "dev:plain-html-components": "npm run dev:html --workspace=@db-ux/core-components",
    "dev:react-components": "npm run dev:react --workspace=@db-ux/core-components",
    "dev:sass": "npm run dev:scss --workspace=@db-ux/core-components",
    "dev:stencil-components": "npm run dev:stencil --workspace=@db-ux/core-components",
    "dev:vue-components": "npm run dev:vue --workspace=@db-ux/core-components",
    "pretest:docs-file-references": "npm run build",
    "rm-builds:components-build": "rimraf ../packages/components/build",
    "rm-builds:dist": "rimraf --glob showcases/**/dist",
    "rm-builds:foundations-build": "rimraf ../packages/foundations/build",
    "rm-builds:output-dist": "rimraf --glob ../output/**/dist",
    "rm-builds:output-playwright": "rimraf --glob ../output/**/playwright",
    "rm-builds:outputs": "rimraf --glob ../output/**/src",
    "rm-builds:patternhub-out": "rimraf --glob showcases/patternhub/out",
    "rm-builds:patternhub-pages-code": "rimraf --glob ../showcases/patternhub/pages/components/**/code",
    "rm-builds:patternhub-pages-docs": "rimraf --glob ../showcases/patternhub/pages/components/**/docs",
    "rm-builds:patternhub-pages-htu": "rimraf --glob ../showcases/patternhub/pages/components/**/how-to-use.mdx",
    "rm-builds:patternhub-pages-index": "rimraf --glob ../showcases/patternhub/pages/components/**/index.tsx",
    "rm-builds:patternhub-pages-migration": "rimraf --glob ../showcases/patternhub/pages/components/**/migration.mdx",
    "rm-builds:patternhub-pages-overview": "rimraf --glob ../showcases/patternhub/pages/components/**/overview.tsx",
    "rm-builds:patternhub-pages-properties": "rimraf --glob ../showcases/patternhub/pages/components/**/properties.mdx",
    "rm-builds:patternhub-src-ts": "rimraf --glob ../showcases/patternhub/components/src/**/*.ts",
    "rm-builds:patternhub-src-tsx": "rimraf --glob ../showcases/patternhub/components/src/**/*.tsx",
    "rm-builds:playwright": "rimraf --glob output/react/playwright",
    "rm-builds:showcases-build": "rimraf --glob  build-showcases/**",
    "rm:builds": "npm-run-all --parallel rm-builds:*",
    "start-showcase:angular": "npm run dev --workspace=angular-showcase",
    "start-showcase:angular-ssr": "npm run dev --workspace=angular-ssr-showcase",
    "start-showcase:next": "npm run dev --workspace=next-showcase",
    "start-showcase:nuxt": "npm run dev --workspace=nuxt-showcase",
    "start-showcase:patternhub": "npm run dev --workspace=patternhub",
    "start-showcase:react": "npm run dev --workspace=react-showcase",
    "start-showcase:stencil": "npm run dev --workspace=stencil-showcase",
    "start-showcase:vue": "npm run dev --workspace=vue-showcase",
    "start:foundations": "npm run start --workspace=@db-ux/core-foundations",
    "test": "npm-run-all --parallel test:*",
    "test:agent-cli": "npm run test --workspace=@db-ux/agent-cli",
    "test:docs-file-references": "node check-docs.js",
    "test:migration": "npm run test --workspace=@db-ux/core-migration",
    "test:scripts": "vitest run --config vitest.config.ts",
    "update:icon-fonts": "npm-run-all generate:icon-fonts generate:icon-types"
  },
  "dependencies": {
    "postcss-scss": "4.0.9"
  },
  "devDependencies": {
    "@inquirer/prompts": "8.2.0",
    "find-versions": "6.0.0",
    "glob": "13.0.1",
    "inquirer": "13.2.2",
    "replace-in-file": "8.4.0",
    "sassdoc": "2.7.4",
    "tar": "7.5.7",
    "typedoc": "0.28.16",
    "typedoc-plugin-markdown": "4.9.0",
    "typedoc-plugin-merge-modules": "7.0.0",
    "vitest": "3.2.4"
  }
}
