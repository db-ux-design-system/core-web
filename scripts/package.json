{
  "name": "scripts",
  "version": "0.0.0",
  "type": "module",
  "description": "contains scripts for monorepo",
  "scripts": {
    "build": "npm-run-all build:foundations build:components build:migration",
    "build-outputs": "npm-run-all -p build-outputs:*",
    "build-outputs:components": "npm run copy-output -w=@db-ui/components",
    "build-outputs:foundations": "npm run copy-build -w=@db-ui/foundations",
    "build-outputs:migration": "npm run copy-build -w=@db-ui/migration",
    "build-outputs:ngx-components": "npm run build -w=@db-ui/ngx-components",
    "build-outputs:react-components": "npm run build -w=@db-ui/react-components",
    "build-outputs:stencil-components": "npm run build -w=@db-ui/web-components",
    "build-outputs:stylelint": "npm run copy-output -w=@db-ui/stylelint",
    "build-outputs:v-components": "npm run build -w=@db-ui/v-components",
    "build-outputs:web-components": "npm run build -w=@db-ui/web-components",
    "build-showcases": "npm-run-all -p build-showcases:*",
    "build-showcases:angular": "npm run build -w=angular-showcase",
    "build-showcases:angular-ssr": "npm run build -w=angular-ssr-showcase",
    "build-showcases:next": "npm run build -w=next-showcase",
    "build-showcases:nuxt": "npm run generate -w=nuxt-showcase",
    "build-showcases:patternhub": "npm run build -w=patternhub",
    "build-showcases:react": "npm run build -w=react-showcase",
    "build-showcases:stencil": "npm run build -w=stencil-showcase",
    "build-showcases:vue": "npm run build -w=vue-showcase",
    "build:components": "npm run build -w=@db-ui/components",
    "build:foundations": "npm run build -w=@db-ui/foundations",
    "build:migration": "npm run build -w=@db-ui/migration",
    "build:stylelint": "npm run build -w=@db-ui/stylelint",
    "dev": "tsx dev.js",
    "dev:angular-components": "npm run dev:angular -w=@db-ui/components",
    "dev:plain-html-components": "npm run dev:html -w=@db-ui/components",
    "dev:react-components": "npm run dev:react -w=@db-ui/components",
    "dev:sass": "npm run dev:scss -w=@db-ui/components",
    "dev:stencil-components": "npm run dev:stencil -w=@db-ui/components",
    "dev:vue-components": "npm run dev:vue -w=@db-ui/components",
    "rm-builds:components-build": "rimraf ../packages/components/build",
    "rm-builds:dist": "rimraf -g showcases/**/dist",
    "rm-builds:foundations-build": "rimraf ../packages/foundations/build",
    "rm-builds:output-dist": "rimraf -g ../output/**/dist",
    "rm-builds:output-playwright": "rimraf -g ../output/**/playwright",
    "rm-builds:outputs": "rimraf -g ../output/**/src",
    "rm-builds:patternhub-out": "rimraf -g showcases/patternhub/out",
    "rm-builds:patternhub-pages-code": "rimraf -g ../showcases/patternhub/pages/components/**/code",
    "rm-builds:patternhub-pages-docs": "rimraf -g ../showcases/patternhub/pages/components/**/docs",
    "rm-builds:patternhub-pages-htu": "rimraf -g ../showcases/patternhub/pages/components/**/how-to-use.mdx",
    "rm-builds:patternhub-pages-index": "rimraf -g ../showcases/patternhub/pages/components/**/index.tsx",
    "rm-builds:patternhub-pages-migration": "rimraf -g ../showcases/patternhub/pages/components/**/migration.mdx",
    "rm-builds:patternhub-pages-overview": "rimraf -g ../showcases/patternhub/pages/components/**/overview.tsx",
    "rm-builds:patternhub-pages-properties": "rimraf -g ../showcases/patternhub/pages/components/**/properties.mdx",
    "rm-builds:patternhub-src-ts": "rimraf -g ../showcases/patternhub/components/src/**/*.ts",
    "rm-builds:patternhub-src-tsx": "rimraf -g ../showcases/patternhub/components/src/**/*.tsx",
    "rm-builds:playwright": "rimraf -g output/react/playwright",
    "rm-builds:showcases-build": "rimraf -g  build-showcases/**",
    "rm:builds": "npm-run-all -p rm-builds:*",
    "start-showcase:angular": "npm run dev -w=angular-showcase",
    "start-showcase:angular-ssr": "npm run dev -w=angular-ssr-showcase",
    "start-showcase:next": "npm run dev -w=next-showcase",
    "start-showcase:nuxt": "npm run dev -w=nuxt-showcase",
    "start-showcase:patternhub": "npm run dev -w=patternhub",
    "start-showcase:react": "npm run dev -w=react-showcase",
    "start-showcase:stencil": "npm run dev -w=stencil-showcase",
    "start-showcase:vue": "npm run dev -w=vue-showcase",
    "start:foundations": "npm run start -w=@db-ui/foundations",
    "test": "npm-run-all -p test:*",
    "test:migration": "npm run test -w=@db-ui/migration",
    "update:icon-fonts": "npm-run-all generate:icon-fonts generate:icon-types"
  },
  "devDependencies": {
    "@inquirer/prompts": "7.2.3",
    "glob": "^11.0.1",
    "inquirer": "^12.3.2",
    "replace-in-file": "^8.3.0"
  }
}
