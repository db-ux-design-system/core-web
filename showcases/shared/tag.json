	{
		"name": "State",
		"examples": [
			{
				"name": "(Default) Unchecked",
				"props": {
					"component": "checkbox"
				},
				"code": {
					"angular": "<db-tag><label><input type=\"checkbox\"/>Interactive Checkbox</label></db-tag>",
					"html": "<div class=\"db-tag\"><label><input type=\"checkbox\"/>Interactive Checkbox</label></div>",
					"react": "<DBTag><label><input type=\"checkbox\"/>Interactive Checkbox</label></DBTag>",
					"vue": "<DBTag><label><input type=\"checkbox\"/>Interactive Checkbox</label></DBTag>"
				}
			},
			{
				"name": "Checked",
				"props": {
					"component": "checkbox",
					"checked": true
				},
				"code": {
					"angular": "<db-tag><label><input type=\"checkbox\" [checked]=\"true\"/>Interactive Checkbox</label></db-tag>",
					"html": "<div class=\"db-tag\"><label><input type=\"checkbox\" checked=\"true\"/>Interactive Checkbox</label></div>",
					"react": "<DBTag><label><input type=\"checkbox\" checked/>Interactive Checkbox</label></DBTag>",
					"vue": "<DBTag><label><input type=\"checkbox\" :checked=\"true\"/>Interactive Checkbox</label></DBTag>"
				}
			},
			{
				"name": "Disabled",
				"props": {
					"component": "checkbox",
					"checked": true,
					"disabled": true
				},
				"code": {
					"angular": "<db-tag><label><input type=\"checkbox\" [checked]=\"true\" [disabled]=\"true\"/>Interactive Checkbox</label></db-tag>",
					"html": "<div class=\"db-tag\"><label><input type=\"checkbox\" checked=\"true\" disabled=\"true\"/>Interactive Checkbox</label></div>",
					"react": "<DBTag><label><input type=\"checkbox\" checked disabled/>Interactive Checkbox</label></DBTag>",
					"vue": "<DBTag><label><input type=\"checkbox\" :checked=\"true\" :disabled=\"true\"/>Interactive Checkbox</label></DBTag>"
				}
			}
		]
