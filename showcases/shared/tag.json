			{
				"name": "Unchecked (Default)",
				"props": {
					"component": "checkbox"
				},
				"code": {
					"angular": "<db-tab><db-checkbox>Interactive Checkbox</db-checkbox></db-tag>",
					"html": "<div class=\"db-tab\"><input type=\"checkbox\" class=\"db-checkbox\" id=\"checkbox-element\" /><label for=\"checkbox-element\">Interactive Checkbox</label></div>",
