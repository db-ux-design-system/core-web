    "@angular/platform-browser": "19.2.4",
    "@angular/platform-browser-dynamic": "19.2.4",
    "@angular/router": "19.2.4",
    "@db-ux/db-theme": "1.0.2",
    "rxjs": "~7.8.2",
    "sa11y": "4.1.5",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^19.2.5",
    "@angular/cli": "19.2.5",
    "@angular/compiler-cli": "19.2.4",
    "ng-packagr": "19.2.0",
