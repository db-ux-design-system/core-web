    "@angular/platform-browser-dynamic": "18.2.13",
    "@angular/router": "18.2.13",
    "@db-ux/db-theme": "1.0.2",
    "rxjs": "~7.8.2",
    "sa11y": "4.1.3",
    "tslib": "^2.8.1",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.16",
    "@angular/cli": "18.2.16",
