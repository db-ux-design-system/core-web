{
  "name": "angular-ssr-showcase",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build && cpr ./dist/angular-ssr-showcase/ ../../build-showcases/angular-ssr-showcase/ -d",
    "build:debug": "ng build --optimization=false",
    "dev": "npm run start -- --open",
    "ng": "ng",
    "serve:ssr:angular-ssr-showcase": "node dist/angular-ssr-showcase/server/server.mjs",
    "start": "ng serve --serve-path angular-ssr-showcase --port 4401",
    "watch": "ng build --watch --configuration development"
  },
  "dependencies": {
    "@angular/animations": "19.1.3",
    "@angular/common": "19.1.3",
    "@angular/compiler": "19.1.3",
    "@angular/core": "19.1.3",
    "@angular/forms": "19.1.3",
    "@angular/platform-browser": "19.1.3",
    "@angular/platform-browser-dynamic": "19.1.3",
    "@angular/platform-server": "19.1.3",
    "@angular/router": "19.1.3",
    "@angular/ssr": "19.1.4",
    "@db-ux/db-theme": "0.3.0",
    "express": "^4.21.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.4",
    "@angular/cli": "19.1.4",
    "@angular/compiler-cli": "19.1.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.10",
    "cpr": "3.0.1",
    "ng-packagr": "19.1.1",
    "typescript": "^5.7.3"
  }
}
