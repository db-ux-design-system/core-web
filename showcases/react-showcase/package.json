{
  "name": "react-showcase",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:01_tsc": "tsc",
    "build:02_vite": "vite build",
    "build:03_copy": "cpr dist ../../build-showcases/react-showcase -o",
    "build:04_copy": "cpr dist dist/react-showcase -o",
    "dev": "vite --open",
    "prestart:e2e": "npm run build:02_vite && npm run build:04_copy",
    "start": "vite",
    "start:e2e": "npx http-server dist",
    "test:e2e": "cross-env showcase=react-showcase npx playwright test --config=../playwright.config.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.8.1"
  },
  "devDependencies": {
    "@playwright/test": "1.30.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "sass": "^1.58.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.3"
  }
}
