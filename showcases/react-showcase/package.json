{
  "name": "react-showcase",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:*",
    "build:01_tsc": "tsc",
    "build:02_vite": "vite build",
    "build:03_copy": "cpr dist ../../build-showcases/react-showcase -o",
    "dev": "vite --open",
    "preview": "npx http-server ../../build-showcases",
    "regenerate:screenshots": "npm-run-all build -p preview test:e2e-regenerate",
    "start": "vite",
    "test:e2e": "cross-env showcase=react-showcase npx playwright test --config=../playwright.config.ts",
    "test:e2e-regenerate": "cross-env showcase=react-showcase npx playwright test -c ../playwright.config.ts --update-snapshots",
    "test:local": "npm-run-all build -p preview test:e2e"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.10.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-env": "^7.0.3",
    "sass": "^1.62.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.1"
  }
}
