{
  "name": "db-patternhub",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "npm-run-all copy build:*",
    "build:app": "next build && next export",
    "build:copy": "cpr out ../../build-showcases/patternhub -o",
