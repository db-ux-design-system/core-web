{
  "name": "@db-ui/stylelint",
  "version": "0.0.0",
  "type": "module",
  "description": "Ensures DB UX styles are used correctly",
  "repository": {
    "type": "git",
    "url": "https://github.com/db-ui/mono.git"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": {
      "types": "./build/src/index.d.ts",
      "default": "./build/src/index.js"
    }
  },
  "files": [
    "build"
  ],
  "keywords": [
    "stylelint",
    "stylelint-plugin"
  ],
  "scripts": {
