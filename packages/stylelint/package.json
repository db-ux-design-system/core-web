  },
  "peerDependencies": {
    "stylelint": "16.13.2"
  },
  "devDependencies": {
    "postcss-html": "^1.8.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.4.5",
    "vitest": "^3.0.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
