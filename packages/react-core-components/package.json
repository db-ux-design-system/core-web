{
  "name": "@db-ux/react-core-components",
  "version": "4.4.2",
  "description": "React components for @db-ux/core-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/db-ux-design-system/core-web.git"
  },
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "files": [
    "CHANGELOG.md",
    "agent",
    "dist/"
  ],
  "scripts": {
    "build": "tsc --project . --sourceMap false",
    "open:report": "npx playwright show-report",
    "regenerate:screenshots": "playwright test --config playwright.config.ts --update-snapshots",
    "test:components": "playwright test --config playwright.config.ts",
    "test:components:ui": "playwright test --config playwright.config.ts --ui",
    "test:windows": "playwright test --config playwright.screen-reader.win.ts"
  },
  "dependencies": {
    "@db-ux/core-components": "workspace:*",
    "@db-ux/core-foundations": "workspace:*"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "1.58.2",
    "@types/react": "19.2.13",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false
}
