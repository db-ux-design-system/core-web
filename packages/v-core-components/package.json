{
  "name": "@db-ux/v-core-components",
  "version": "4.4.0",
  "type": "module",
  "description": "Vue components for @db-ux/core-components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/db-ux-design-system/core-web.git"
  },
  "license": "Apache-2.0",
  "main": "./dist/db-ux.umd.js",
  "module": "./dist/db-ux.es.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/db-ux.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/db-ux.umd.js"
      }
    }
  },
  "files": [
    "CHANGELOG.md",
    "agent",
    "dist/"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:01_vite": "vite build",
    "build:02_types": "vue-tsc --declaration --emitDeclarationOnly",
    "test:components": "playwright test --config playwright.config.ts",
    "test:components:ui": "playwright test --config playwright.config.ts --ui"
  },
  "dependencies": {
    "@db-ux/core-components": "workspace:*",
    "@db-ux/core-foundations": "workspace:*"
  },
  "devDependencies": {
    "@playwright/experimental-ct-vue": "1.57.0",
    "@vitejs/plugin-vue": "6.0.4",
    "replace-in-file": "8.4.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vue": "3.5.27",
    "vue-tsc": "3.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "sideEffects": false,
  "source": "src/index.ts"
}
